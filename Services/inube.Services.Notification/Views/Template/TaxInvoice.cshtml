                            <ol style="padding: 0"><span>Bank Name :</span> <strong>@Html.DisplayFor(model => model.BankDetails.BankName)</strong></ol>
                            <ol style="padding: 0"><span>A/C No :</span> <strong>   A/C No:@Html.DisplayFor(model => model.BankDetails.AccNo)</strong></ol>
                            <ol style="padding: 0">
                                <span>Branch & IFS Code :</span>
                                <strong>
                                    @Html.DisplayFor(model => model.BankDetails.Branch)
                                    @Html.DisplayFor(model => model.BankDetails.IFSC)
                                </strong>
                            </ol>
                        </ul>
                    </td>
                </tr>

                <tr>
                    <td style="vertical-align: top;padding: 5px;">
                        <span>Company’s PAN:</span>
                        <strong>@Html.DisplayFor(model => model.BankDetails.CompanysPAN)</strong>
                    </td>
                    <td style="border: 1px solid #000; height: 70px;vertical-align: top; padding: 5px;">
