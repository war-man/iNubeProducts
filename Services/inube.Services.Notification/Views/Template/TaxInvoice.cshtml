<body>
    <!--style="background-image:url(/images/background.png); background-repeat:repeat;background-attachment:fixed;background-size:cover;background-position:center;"-->
    <div class="container" style="padding: 20px;">
        <h2 style="text-align: center">Tax Invoice</h2>
        <table class="table table-condensed" style="width:100%;border: 1px solid #000;padding: 5px; background-image:url(/images/watermark.jpg);background-repeat:repeat">
            <tbody>
                <tr style="width: 100%">
                    <td style="width: 50%; vertical-align: top; padding: 5px;border: 1px solid #000;">
                        <img src="~/images/iNubeLogo.png" style="float: left;">
                        <p style="float: left; margin-left: 20px; line-height:25px;">

                            <b> @Html.DisplayFor(model => model.Address.OfficeName)</b><br />
                            @Html.DisplayFor(model => model.Address.AddressLine1)<br />
                            @Html.DisplayFor(model => model.Address.AddressLine2)<br>
                            @Html.DisplayFor(model => model.Address.City) 560078
                            @Html.DisplayFor(model => model.Address.State)
                            StateCode:@Html.DisplayFor(model => model.Address.Code)<br>
                            GSTIN/UIN:@Html.DisplayFor(model => model.Address.UIN)<br />
                            <!--State Name :  @Html.DisplayFor(model => model.Address.State),
                            Code:@Html.DisplayFor(model => model.Address.Code)<br>-->
                            CIN:@Html.DisplayFor(model => model.Address.CIN)
                        </p>
                    </td>

                    <td rowspan="2" style="width: 50%; border: 1px solid #000;">
                        <table style="width: 100%">
                            <tbody>

                                <tr>
                                    <td style="width: 50%;padding: 5px;border: 1px solid #000;">
                                        <span>Invoice No.</span>
                                        <span style="float: right;">e-Way Bill No.</span><br>
                                        <strong> @Html.DisplayFor(model => model.InvoiceItemsModel.InvoiceNo)</strong>
                                    </td>
                                    <td style="width: 50%;padding: 5px;border: 1px solid #000;">
                                        <span>
                                            Dated<span>
                                                <br>
                                                <strong>@Html.DisplayFor(model => model.InvoiceItemsModel.InvoiceDate)</strong>
                                    </td>
                                </tr>

                                <tr>
                                    <td style="width: 50%;padding: 5px;border: 1px solid #000;"></td>
                                    <td style="width: 50%;padding: 5px;border: 1px solid #000;">
                                        <span>
                                            Mode/Terms of Payment<span>
                                                <br>
                                                <strong>  @Html.DisplayFor(model => model.InvoiceItemsModel.Days) Days</strong>
                                    </td>
                                </tr>

                                <tr>
                                    <td style="width: 50%;padding: 5px;border: 1px solid #000;">
                                        <span>Supplier’s Ref.</span><br>
                                        <strong> @Html.DisplayFor(model => model.InvoiceItemsModel.SuppliesrsRef)</strong>
                                    </td>

                                    <td style="width: 50%;padding: 5px;border: 1px solid #000;">
                                        <span>
                                            Other Reference(s)<span>
                                                <br>
                                                <Strong> @Html.DisplayFor(model => model.BuyersAddress.OthersRef) </Strong>
                                    </td>
                                </tr>

                                <tr>
                                    <td style="width: 50%;padding: 5px;border: 1px solid #000;">
                                        <span>Buyer’s Order No.</span><br>
                                        <strong>  @Html.DisplayFor(model => model.InvoiceItemsModel.OrderNo)</strong>
                                    </td>
                                    <td style="width: 50%;padding: 5px;border: 1px solid #000;">
                                        <span>
                                            Dated<span>
                                                <br>
                                                <strong> @Html.DisplayFor(model => model.InvoiceItemsModel.PODate)</strong>
                                    </td>
                                </tr>

                                <tr style="width: 100%">
                                    <td colspan="2" style="padding: 5px;border: 1px solid #000;height: 120px;vertical-align: baseline;">
                                        <span>Terms of Delivery</span><br>
                                        <strong>@Html.DisplayFor(model => model.InvoiceItemsModel.TermsofDelivery)</strong>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                </tr>

                <tr>
                    <td style="width: 50%; vertical-align: top; padding: 5px;border: 1px solid #000;line-height:25px;">
                        <span>Buyer</span><br>
                        <Strong>@Html.DisplayFor(model => model.BuyersAddress.Buyer)</Strong><br>
                        <p>
                            @Html.DisplayFor(model => model.BuyersAddress.AddressLine1),<br>
                            @Html.DisplayFor(model => model.BuyersAddress.AddressLine2)<br>GSTIN/UIN : 27AABCF0191R2Z8<br>
                            PAN/IT No:@Html.DisplayFor(model => model.BuyersAddress.PAN)<br>
                            State Name : @Html.DisplayFor(model => model.BuyersAddress.State),
                            Code :@Html.DisplayFor(model => model.BuyersAddress.Code)
                        </p>
                    </td>
                </tr>
