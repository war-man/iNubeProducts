{
  "family": "ed-prod-mica-notification",
  "taskRoleArn": "",
  "executionRoleArn": "arn:aws:iam::471379395009:role/ECSTaskDefinitionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "ed-prod-mica-notification",
      "image": "<ednotification>",

      "cpu": 800,
      "memory": 1500,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ed-prod-mica-notification",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      },

      "portMappings": [
        {
          "containerPort": 9004,
          "hostPort": 9004,
          "protocol": "tcp"
        }
      ],
      "essential": true

    }
  ],


  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "800",
  "memory": "1500",
  "logConfiguration": {
    "logDriver": "awslogs",
    "options": {
      "awslogs-group": "/ecs/ed-prod-mica-notification",
      "awslogs-region": "ap-south-1",
      "awslogs-stream-prefix": "ecs"
    }
  }

}