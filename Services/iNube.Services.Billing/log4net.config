<?xml version="1.0" encoding="utf-8" ?>
<log4net>
	<appender name="RollingFile" type="log4net.Appender.FileAppender">
		<file value="E:\Temp\LogFiles\LogFile.log" />
		<layout type="log4net.Layout.PatternLayout">
		<conversionPattern value="%date %d{hh:mm:ss} [%thread] %-5level %logger - %message%newline"/> 
		</layout>
	</appender>
	<root>
		<level value="ALL" />
		<appender-ref ref="RollingFile"/>
	</root>
</log4net>