{
  "executionRoleArn": "arn:aws:iam::820227512150:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "dev2-mica-billing",
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 9001,
          "protocol": "tcp",
          "containerPort": 9001
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "networkMode": "awsvpc",
  "cpu": "300",
  "memory": "300",
  "family": "dev2-mica-billing"
}