{
  "family": "ed-prod-mica-user",
  "taskRoleArn": "",
  "executionRoleArn": "arn:aws:iam::471379395009:role/ECSTaskDefinitionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "ed-prod-mica-user",
      "image": "<eduser>",

      "cpu": 1024,
      "memory": 3048,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ed-prod-mica-user",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      },

      "portMappings": [
        {
          "containerPort": 9009,
          "hostPort": 9009,
          "protocol": "tcp"
        }
      ],
      "essential": true

    }
  ],


  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "1024",
  "memory": "3048",
  "logConfiguration": {
    "logDriver": "awslogs",
    "options": {
      "awslogs-group": "/ecs/ed-prod-mica-user",
      "awslogs-region": "ap-south-1",
      "awslogs-stream-prefix": "ecs"
    }
  }

}