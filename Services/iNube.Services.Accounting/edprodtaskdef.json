{
  "family": "ed-prod-mica-accounting",
  "taskRoleArn": "",
  "executionRoleArn": "arn:aws:iam::471379395009:role/ECSTaskDefinitionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "ed-prod-mica-accounting",
      "image": "<edaccounting>",

      "cpu": 512,
      "memory": 1024,

      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ed-prod-mica-accounting",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "containerPort": 9011,
          "hostPort": 9011,
          "protocol": "tcp"
        }
      ],
      "essential": true

    }

  ],

  "requiresCompatibilities": [ "EC2" ],
  "cpu": "512",
  "memory": "1024",
  "logConfiguration": {
    "logDriver": "awslogs",
    "options": {
      "awslogs-group": "/ecs/ed-prod-mica-accounting",
      "awslogs-region": "ap-south-1",
      "awslogs-stream-prefix": "ecs"
    }
  }

}