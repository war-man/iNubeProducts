{
  "family": "dev2-mica-accounting",
  "taskRoleArn": "",
  "executionRoleArn": "arn:aws:iam::820227512150:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "dev2-mica-accounting",
      "image": "<IMAGE1_NAME>",

      "cpu": 512,
      "memory": 512,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/dev2-mica-accounting",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs",
        }
      },

          "portMappings": [
            {
              "containerPort": 9011,
              "hostPort": 9011,
              "protocol": "tcp"
            }
          ],
          "essential": true

        },



        "requiresCompatibilities": [
          "EC2"
        ],
        "cpu": "512",
        "memory": "512"

      }