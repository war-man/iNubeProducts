
{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {"node": "10"}
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      { "root": [ "./src" ] }
    ],

    "babel-plugin-transform-dynamic-import",
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
   
  ]
}